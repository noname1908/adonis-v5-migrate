{
  "name": "adonis-migrate",
  "version": "0.0.0",
  "description": "A tool for migrating Adonisjs application from v4 to v5",
  "license": "MIT",
  "contributors": [
    {
      "name": "Duy Nguyen",
      "url": "https://github.com/noname1908"
    }
  ],
  "homepage": "https://github.com/noname1908/adonis-v5-migrate#readme",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "adonis-v5-migrate": "build/cli.js",
    "adonis-v5-migrate-full": "bin/adonis-migrate-full.sh"
  },
  "keywords": [
    "adonis-migrate",
    "adonis-v5-migrate"
  ],
  "files": [
    "build",
    "bin",
    "package.json",
    "README.md",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/noname1908/adonis-v5-migrate.git"
  },
  "bugs": {
    "url": "https://github.com/noname1908/adonis-v5-migrate/issues"
  },
  "scripts": {
    "test": "jest --config jest-config.json",
    "lint": "eslint . --ext=.js,.ts --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext=.js,.ts --ignore-path .eslintignore --fix",
    "build": "tsc -p tsconfig.json",
    "prepublish": "npm run build",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "create-jest-runner": "^0.11.1",
    "jscodeshift": "^0.12.0",
    "json5": "^2.2.1",
    "json5-writer": "^0.1.8",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "ts-migrate-plugins": "^0.1.28",
    "ts-migrate-server": "^0.1.27",
    "ts-node": "^8.3.0",
    "updatable-log": "^0.2.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "adonis-preset-ts": "^2.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "glob": "^8.0.3",
    "jest": "^26.4.0",
    "react": "^18.2.0",
    "ts-jest": "^26.3.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "typescript": ">4.0"
  }
}
